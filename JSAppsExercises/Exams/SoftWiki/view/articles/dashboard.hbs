{{>header}}
<!-- Dashboard -->
    <!-- Home -->    
    <div class="content">
        
      <section class="js">
        <h2>JavaScript</h2>
        {{#each JSArticles}}
        <div class="articles">
          <article>
            <h3>{{title}}</h3>
            <p>
             {{content}}
            </p>
            <a href="#/articles/details/{{id}}" class="btn details-btn">Details</a>
          </article>              
        </div>  
          {{else}}         
         <h3 class="no-articles">No articles yet</h3>
          {{/each}}           
      </section>
    
